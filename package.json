{
  "name": "supernote-companion",
  "version": "1.0.1",
  "description": "Sync Supernote .note files to your Obsidian vault as PDFs with structured markdown entries",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "supernote",
    "pdf",
    "sync",
    "e-ink"
  ],
  "author": "dpshade",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/pako": "^2.0.4",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "pako": "^2.1.0"
  }
}
